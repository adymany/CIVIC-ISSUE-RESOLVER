{
  "name": "civic-reporting-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "dev:https:lan": "next dev --turbopack --experimental-https -H 0.0.0.0 -p 3001",
    "dev:https:custom": "node server.js",
    "dev:tunnel": "node dev-with-tunnel.js",
    "dev:cloudflare": "cloudflared tunnel --url http://localhost:3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "node prisma/seed.js",
    "setup-db": "node setup-database.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "bcryptjs": "^2.4.3",
    "leaflet": "^1.9.4",
    "next": "15.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "prisma": "^6.16.1"
  }
}